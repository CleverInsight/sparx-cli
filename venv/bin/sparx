#!/Users/bastinrobins/Documents/tools/sparx-io/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'Sparx-IO','console_scripts','sparx'
__requires__ = 'Sparx-IO'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('Sparx-IO', 'console_scripts', 'sparx')()
    )
